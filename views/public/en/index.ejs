<section class="quote-area bg-item ptb-100">
    <div class="container">
        <div class="quote-item item-two">
            <div class="content">
                <span>Get a Quote</span>
                <h3>Pre-enrollment in classes</h3>
            </div>
            <form action="/en/classRegister" method="post">
                <%- include('../../layouts/validationError' , {Errors : ClientError}) %>

                    <div class="form-group">
                        <input type="text" class="form-control" name="username" placeholder="First and last name">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" name="age" placeholder="Student age">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" name="nationalId" placeholder="National Id">
                    </div>
                    <div class="form-group row">
                        <div class="col">
                            <label for="category" class="control-label font-weight-bold font-weight-bold">For class:
                            </label>
                            <select name="classe" id="category" class="form-control">
                                <% classes.forEach((classe)=>{%>
                                    <option value="<%= classe._id %>">
                                        <%= classe.title %>
                                    </option>
                                    <%})%>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" name="fatherName" placeholder="Father's name">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" name="fatherPhoneNumber"
                            placeholder="Father's phone number">
                    </div>
                    <div class="form-group">
                        <input type="text" class="form-control" name="matherPhoneNumber"
                            placeholder="Mother's phone number">
                    </div>
                    <button type="submit" class="default-btn">
                        Submit Now
                    </button>
            </form>
        </div>
    </div>
</section>
