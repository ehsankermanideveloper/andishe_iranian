<div class="row">
    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
        <div class="card">
            <h5 class="card-header">زبان ها</h5>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered first">
                        <thead>

                            <tr>
                                <th>عنوان حریم خصوصی</th>
                                <th>تنظیمات</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% privacys.forEach((privacy)=>{%>
                                <tr>
                                    <td>
                                        <%=privacy.title%>
                                    </td>
                                    <td>
                                        <div>
                                            <a href="/admin/privacys/<%=privacy.id%>/edit"
                                                class="btn btn-sm btn-primary">ویرایش</a>
                                            <form action="/admin/privacys/<%= privacy._id %>/remove?_method=DELETE"
                                                  method="post">
                                                <button class="btn btn-sm btn-danger">حذف</button>
                                            </form>
                                        </div>
                                    </td>
                                </tr>
                                <%})%>
                        </tbody>
                    </table>
                    <br><br>
                </div>
            </div>
        </div>
    </div>
</div>