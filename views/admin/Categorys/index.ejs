<div class="row">
    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
        <div class="card">
            <h5 class="card-header">دسته بندی ها</h5>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered first">
                        <thead>

                            <tr>
                                <th>عنوان دسته بندی</th>
                                <th>برای وب سایت با زبان : </th>
                                <th>تنظیمات</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% categorys.forEach((cate)=>{%>
                                <tr>
                                    <td>
                                        <%=cate.title%>
                                    </td>
                                    <td>
                                        <%=cate.language.title%>
                                    </td>
                                    <td>
                                        <div>
                                            <a href="/admin/categorys/<%=cate.id%>/edit"
                                                class="btn btn-sm btn-primary">ویرایش</a>
                                        </div>
                                    </td>
                                </tr>
                                <%})%>
                        </tbody>
                    </table>
                    <br><br>
                </div>
            </div>
        </div>
    </div>
</div>
