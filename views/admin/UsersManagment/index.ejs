<div class="row">
    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
        <div class="card">
            <h5 class="card-header">کاربران</h5>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered first">
                        <thead>
                            <tr>
                                <th>نام کاربری</th>
                                <th>کد ملی</th>
                                <th>عضو شده در :</th>
                                <th>تنظیمات</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% users.forEach((user)=>{%>
                                <tr>
                                    <td>
                                        <%= user.username%>
                                    </td>

                                    <td>
                                        <%= user.nationalId %>
                                    </td>

                                    <td>
                                        <%- convertTime(user.createdAt).format('jYYYY/jMM/jD') %>
                                    </td>
                                    <td>
                                        <div>
                                            <a href="/admin/usersmanagment/<%=user._id%>/edit"
                                                class="btn btn-sm btn-primary">ویرایش</a>


                                        </div>
                                    </td>

                                </tr>
                                <% }) %>
                        </tbody>
                    </table>
                    <br><br>
                </div>
            </div>
        </div>
    </div>
</div>