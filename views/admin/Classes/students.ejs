<div class="row">
    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
        <div class="card">
            <h5 class="card-header">دانش آموزان کلاس ها</h5>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered first">
                        <thead>
                            <tr>
                                <th>نام دانش آموز</th>
                                <th>کد ملی دانش آموز</th>
                                <th>سن دانش آموز</th>
                                <th>نام پدر</th>
                                <th>شماره تلفن پدر</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% classeStudents.forEach((student)=>{%>
                                <tr>
                                    <td>
                                        <%=student.username%>
                                    </td>
                                    <td>
                                        <%= student.nationalId %>
                                    </td>
                                    <td>
                                        <%= student.age %>
                                    </td>
                                    <td>
                                        <%= student.fatherName %>
                                    </td>
                                    <td>
                                        <%= student.fatherPhoneNumber %>
                                    </td>
                                </tr>
                                <%})%>
                        </tbody>
                    </table>
                    <br><br>
                </div>
            </div>
        </div>
    </div>
</div>