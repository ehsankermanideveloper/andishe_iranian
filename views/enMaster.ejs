<!doctype html>
<html lang="zxx">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link rel="stylesheet" href="/en/assets/css/bootstrap.min.css">

    <link rel="stylesheet" href="/en/assets/css/animate.min.css">

    <link rel="stylesheet" href="/en/assets/css/meanmenu.css">

    <link rel="stylesheet" href="/en/assets/css/boxicons.min.css">

    <link rel="stylesheet" href="/en/assets/css/owl.carousel.min.css">

    <link rel="stylesheet" href="/en/assets/css/owl.theme.default.min.css">

    <link rel="stylesheet" href="/en/assets/css/odometer.min.css">

    <link rel="stylesheet" href="/en/assets/css/magnific-popup.min.css">

    <link rel="stylesheet" href="/en/assets/css/imagelightbox.min.css">

    <link rel="stylesheet" href="/en/assets/css/style.css">

    <link rel="stylesheet" href="/en/assets/css/responsive.css">
    <title><%- options.title %></title>
    <link rel="icon" type="image/png" href="/en/assets/img/favicon.png">
</head>

<body>


    <div class="preloader">
        <div class="loader">
            <div class="wrapper">
                <div class="circle circle-1"></div>
                <div class="circle circle-1a"></div>
                <div class="circle circle-2"></div>
                <div class="circle circle-3"></div>
            </div>
        </div>
    </div>


    <div class="navbar-area">
        <div class="main-responsive-nav">
            <div class="container">
                <div class="main-responsive-menu">
                    <div class="logo">
                        <a href="index.html">
                            <img src="/en/assets/img/logo.png" alt="image">
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <div class="main-navbar">
            <div class="container">
                <nav class="navbar navbar-expand-md navbar-light">
                    <a class="navbar-brand" href="index.html">
                        <img src="/en/assets/img/logo.png" alt="image">
                    </a>
                    <div class="collapse navbar-collapse mean-menu" id="navbarSupportedContent">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a href="/en" class="nav-link active">
                                    Home
                                </a>

                            </li>
                            <li class="nav-item">
                                <a href="#" class="nav-link">
                                    School Enroll
                                    <i class='bx bx-chevron-down'></i>
                                </a>
                                <ul class="dropdown-menu">
                                    <% classes.forEach((classLis)=>{%>
                                        <li class="nav-item">
                                            <a href="/en/classRegister" class="nav-link">
                                                کلاس    <%= classLis.title %>
                                            </a>
                                        </li>
                                    <%})%>
                                </ul>
                            </li>
                            <li class="nav-item">
                                <a href="/en/blogs" class="nav-link">
                                   Blogs
                                </a>

                            </li>
                            <li class="nav-item">
                                <a href="/en/teachers" class="nav-link">
                                    Tearchers
                                </a>
                            </li>
                            <% if(!user){%>
                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        Accounts
                                        <i class='bx bx-chevron-down'></i>
                                    </a>
                                    <ul class="dropdown-menu">
                                        <li class="nav-item">
                                            <a href="/en/auth/register" class="nav-link">
                                               Register
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a href="/en/auth/login" class="nav-link">
                                                Login
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                            <%}%>
                            <li class="nav-item">
                                <a href="/en/privacy" class="nav-link">
                                    Privacy
                                </a>
                            </li>
                            <li class="nav-item">
                                <a href="#" class="nav-link">
                                    Pages
                                    <i class='bx bx-chevron-down'></i>
                                </a>
                                <ul class="dropdown-menu">
                                    <li class="nav-item">
                                        <a href="/en/about" class="nav-link">
                                        About
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/en/conctact" class="nav-link">
                                            Contact
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/en/gallery" class="nav-link">
                                            Gallery
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                        <div class="others-options d-flex align-items-center">
                            <div class="option-item">
                                <a href="/en" class="dropdown-item d-flex align-items-center default-btn">
                                    <img src="/fa/assets/img/english.png" class="shadow-sm" alt="flag" width="20px" style="margin-right: 5px">
                                    <span>English</span>
                                </a>
                            </div>
                            <div class="option-item">
                                <a href="/fa" class="dropdown-item d-flex align-items-center default-btn">
                                    <img src="/fa/assets/img/iran.png" class="shadow-sm" alt="flag" width="20px" style="margin-right: 5px">
                                    <span style="font-family: 'vazir', sans-serif">Persian</span>
                                </a>
                            </div>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
        <div class="others-option-for-responsive">
            <div class="container">
                <div class="dot-menu">
                    <div class="inner">
                        <div class="circle circle-one"></div>
                        <div class="circle circle-two"></div>
                        <div class="circle circle-three"></div>
                    </div>
                </div>
                <div class="container">
                    <div class="option-inner">
                        <div class="others-options d-flex align-items-center">
                            <div class="option-item">
                                <a href="/en" class="dropdown-item d-flex align-items-center default-btn">
                                    <img src="/fa/assets/img/english.png" class="shadow-sm" alt="flag" width="20px" style="margin-right: 5px">
                                    <span>English</span>
                                </a>
                            </div>
                            <div class="option-item">
                                <a href="/fa" class="dropdown-item d-flex align-items-center default-btn">
                                    <img src="/fa/assets/img/iran.png" class="shadow-sm" alt="flag" width="20px" style="margin-right: 5px">
                                    <span style="font-family: 'vazir', sans-serif">Persian</span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <%- body %>
        <section class="footer-area pt-100 pb-70">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3 col-sm-6">
                        <div class="single-footer-widget">
                            <div class="logo">
                                <h2 style="font-size: 25px">
                                    <a href="/en">Andishe Iranian Schools</a>
                                </h2>
                            </div>
                            <p><%= schoolInfo.infoEn.slice(0,300) %>...</p>

                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <div class="single-footer-widget">
                            <h3>Contact Us</h3>
                            <ul class="footer-contact-info">
                                <li>
                                    <i class='bx bxs-phone'></i>
                                    <span>Phone</span>
                                    <a href="tel:<%= schoolInfo.schoolPhone %>"><%= schoolInfo.schoolPhone %></a>
                                </li>
                                <li>
                                    <i class='bx bx-envelope'></i>
                                    <span>Email</span>
                                    <a href="mailto: <%= schoolInfo.email %>"><%= schoolInfo.email %></a>
                                </li>
                                <li>
                                    <i class='bx bx-map'></i>
                                    <span>Address</span>
                                    <%= schoolInfo.mapEn %>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <div class="single-footer-widget pl-5">
                            <h3>Pages</h3>
                            <ul class="quick-links">
                                <li>
                                    <a href="/en/auth/register">Register</a>
                                </li>
                                <li>
                                    <a href="/en/auth/login">Login</a>
                                </li>
                                <li>
                                    <a href="/en/gallery">Gallery</a>
                                </li>
                                <li>
                                    <a href="/en/about">About</a>
                                </li>
                                <li>
                                    <a href="/en/contact">Contact</a>
                                </li>
                                <li>
                                    <a href="/en/blogs">Blogs</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <div class="single-footer-widget">
                            <h3>Photo Gallery</h3>
                            <ul class="photo-gallery-list">
                                <li>
                                    <div class="box">
                                        <img src="/en/assets/img/footer-gallery/footer-gallery-1.jpg" alt="image">
                                        <a href="#" target="_blank" class="link-btn"></a>
                                    </div>
                                </li>
                                <li>
                                    <div class="box">
                                        <img src="/en/assets/img/footer-gallery/footer-gallery-2.jpg" alt="image">
                                        <a href="#" target="_blank" class="link-btn"></a>
                                    </div>
                                </li>
                                <li>
                                    <div class="box">
                                        <img src="/en/assets/img/footer-gallery/footer-gallery-3.jpg" alt="image">
                                        <a href="#" target="_blank" class="link-btn"></a>
                                    </div>
                                </li>
                                <li>
                                    <div class="box">
                                        <img src="/en/assets/img/footer-gallery/footer-gallery-4.jpg" alt="image">
                                        <a href="#" target="_blank" class="link-btn"></a>
                                    </div>
                                </li>
                                <li>
                                    <div class="box">
                                        <img src="/en/assets/img/footer-gallery/footer-gallery-5.jpg" alt="image">
                                        <a href="#" target="_blank" class="link-btn"></a>
                                    </div>
                                </li>
                                <li>
                                    <div class="box">
                                        <img src="/en/assets/img/footer-gallery/footer-gallery-6.jpg" alt="image">
                                        <a href="#" target="_blank" class="link-btn"></a>
                                    </div>
                                </li>
                                <li>
                                    <div class="box">
                                        <img src="/en/assets/img/footer-gallery/footer-gallery-1.jpg" alt="image">
                                        <a href="#" target="_blank" class="link-btn"></a>
                                    </div>
                                </li>
                                <li>
                                    <div class="box">
                                        <img src="/en/assets/img/footer-gallery/footer-gallery-2.jpg" alt="image">
                                        <a href="#" target="_blank" class="link-btn"></a>
                                    </div>
                                </li>
                                <li>
                                    <div class="box">
                                        <img src="/en/assets/img/footer-gallery/footer-gallery-3.jpg" alt="image">
                                        <a href="#" target="_blank" class="link-btn"></a>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <div class="copyright-area">
            <div class="container">
                <div class="copyright-area-content">
                    <p>
                        Copyright © 2021 Ketan. All Rights Reserved by
                        <a href="https://envytheme.com/" target="_blank">
                            EnvyTheme
                        </a>
                    </p>
                </div>
            </div>
        </div>
        <div class="go-top">
            <i class='bx bx-up-arrow-alt'></i>
        </div>


        <script src="/en/assets/js/jquery.min.js"></script>

        <script src="/en/assets/js/popper.min.js"></script>

        <script src="/en/assets/js/bootstrap.min.js"></script>

        <script src="/en/assets/js/jquery.meanmenu.js"></script>

        <script src="/en/assets/js/owl.carousel.min.js"></script>

        <script src="/en/assets/js/jquery.magnific-popup.min.js"></script>

        <script src="/en/assets/js/imagelightbox.min.js"></script>

        <script src="/en/assets/js/odometer.min.js"></script>

        <script src="/en/assets/js/jquery.appear.min.js"></script>

        <script src="/en/assets/js/jquery.ajaxchimp.min.js"></script>

        <script src="/en/assets/js/form-validator.min.js"></script>

        <script src="/en/assets/js/contact-form-script.js"></script>

        <script src="/en/assets/js/main.js"></script>
</body>

</html>